<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.petrik.bankdesktopapp.Confirmation">
   <children>
      <VBox prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Biztos törölni akarod?" />
            <Button fx:id="confirm" mnemonicParsing="false" onAction="#confirmAction" text="Igen" />
            <Button mnemonicParsing="false" onAction="#cancelAction" text="Nem" />
         </children>
      </VBox>
   </children>
</AnchorPane>
